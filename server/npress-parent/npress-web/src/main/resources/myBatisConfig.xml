<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--settings内可配置myBatis框架运行行为的属性信息-->
    <settings>
        <setting name="lazyLoadingEnabled" value="false"/>
    </settings>

    <!--定义全限定类名的别名-->
    <typeAliases>
        <typeAlias alias="LoginLog" type="com.med.web.domain.LoginLog"/>
        <typeAlias alias="User" type="com.med.web.domain.User"/>
    </typeAliases>

    <!--将myBatis映射文件组装起来-->
    <mappers>
        <mapper resource="org/mybatis/example/BlogMapper.xml"/>
    </mappers>

</configuration>